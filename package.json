{
  "name": "@apraamcos/pass-js",
  "description": "Apple Wallet Pass generating and pushing updates from Node.js",
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "version": "6.9.10",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apraamcos/pass-js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/walletpass/pass-js/blob/master/MIT-LICENSE"
    }
  ],
  "engines": {
    "node": ">=10.10"
  },
  "bin": {
    "passkit-keys": "./bin/passkit-keys"
  },
  "files": [
    "dist/*.js",
    "dist/lib/*.js",
    "dist/*.ts",
    "dist/lib/*.ts",
    "bin/passkit-keys"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "devDependencies": {
    "@types/buffer-crc32": "0.2.4",
    "@types/color-name": "1.1.4",
    "@types/jest": "29.5.12",
    "@types/node": "22.3.0",
    "@types/node-forge": "1.3.11",
    "@types/yauzl": "2.10.3",
    "@typescript-eslint/eslint-plugin": "8.1.0",
    "@typescript-eslint/parser": "8.1.0",
    "@typescript-eslint/typescript-estree": "8.1.0",
    "env-app-yaml": "1.0.0",
    "eslint": "9.9.0",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.9",
    "ts-jest": "29.2.4",
    "typescript": "5.5.4"
  },
  "scripts": {
    "test": "node -r env-app-yaml/config --expose-gc node_modules/jest/bin/jest --detectOpenHandles --logHeapUsage --maxWorkers=1",
    "lint": "eslint \"{src,__tests__}/**/*.ts\" --ignore-pattern \"*test*\" --ignore-path .gitignore",
    "prepublishOnly": "tsc"
  },
  "dependencies": {
    "buffer-crc32": "0.2.13",
    "color-name": "1.1.4",
    "do-not-zip": "1.0.0",
    "event-iterator": "2.0.0",
    "imagesize": "1.0.0",
    "node-forge": "1.3.1",
    "strip-json-comments": "5.0.1",
    "yauzl": "2.10.0"
  }
}
